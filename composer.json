{
    "name": "internetgalerie/ig-mfa-frontend",
    "type": "typo3-cms-extension",
    "description": "MFA for Frontend",
    "authors": [
        {
            "name": "Daniel Abplanalp",
            "role": "Developer"
        },
        {
            "name": "Noah Grossen",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
	"typo3/cms-core": "^12.4"
    },
    "autoload": {
	"psr-4": {
	    "Internetgalerie\\IgMfaFrontend\\": "Classes"
	}
    },
    "replace": {
	"typo3-ter/ig-mfa-frontend": "self.version"
    },
    "config": {
	"vendor-dir": ".Build/vendor",
	"bin-dir": ".Build/bin"
    },
    "scripts": {
	"post-autoload-dump": [
	    "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
	]
    },
    "extra": {
	"typo3/cms": {
	    "cms-package-dir": "{$vendor-dir}/typo3/cms",
	    "web-dir": ".Build/public",
	    "extension-key": "ig_mfa_frontend"
	}
    }
}
